{
  "private": true,
  "name": "unindented-notes-hugo",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/unindented/unindented-notes-hugo.git"
  },
  "scripts": {
    "clean": "rm -rf .packages e2e-report e2e-results public resources/_gen .hugo_build.lock hugo_stats.json",
    "start": "hugo server --buildDrafts --buildExpired --buildFuture --navigateToChanged",
    "build": "hugo --cleanDestinationDir --minify",
    "test:lint": "eslint .",
    "test:type": "tsc -p assets/jsconfig.json",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@eslint/js": "9.7.0",
    "@playwright/test": "1.45.2",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "autoprefixer": "10.4.19",
    "chalk": "^5.3.0",
    "cssnano": "7.0.4",
    "eslint": "9.7.0",
    "globals": "15.8.0",
    "postcss": "8.4.39",
    "prettier": "3.3.3",
    "prettier-plugin-go-template": "0.0.15",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rollup": "4.18.1",
    "tailwindcss": "3.4.6",
    "typescript": "5.5.3"
  },
  "packageManager": "pnpm@9.5.0"
}
